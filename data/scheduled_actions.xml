<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Scheduled Action to archive temporary groups -->
        <record id="ir_cron_archive_expired_temporary_access_groups" model="ir.cron">
            <field name="name">Archive Expired Temporary Accessible Groups</field>
            <field name="model_id" ref="model_tk_accessible_group"/>
            <field name="state">code</field>
            <field name="code">model._archive_expired_groups()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
